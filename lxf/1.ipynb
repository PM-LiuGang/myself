    "        @functools.wraps(func)\n",
    "        def wrapper(*args,**kw):\n",
    "            print(func.__name__,' begin ' , *text)\n",
    "            func(*args,**kw)\n",
    "            print(func.__name__,' end ', *text)\n",
    "        return wrapper\n",
    "    return decorator\n",
    "\n",
    "@log('excute')  #or@log()\n",
    "def foo():\n",
    "    print('foo is running...')\n",
    "if __name__=='__main__':\n",
    "    f=foo\n",
    "    f()"
   ]
