    "def _odd_iter_():\n",
    "    # 生成器，并且是一个无限序列\n",
    "    n = 1\n",
    "    while True:\n",
    "        n = n + 2 # 从3开始\n",
    "        yield n\n",
    "        \n",
    "def _not_divisible(n):\n",
    "    # 筛选函数，不能被整除\n",
    "    return lambda x: x % n > 0\n",
    "    \n",
    "def primes():\n",
    "    # 生成器，不断返回下一个素数\n",
    "    yield 2\n",
    "    it = _odd_iter_() # 初始序列\n",
    "    while True:\n",
    "        n = next(it) # 返回序列的第一个数\n",
    "        yield n \n",
    "        it = filter(_not_divisible(n), it)"
   ]
