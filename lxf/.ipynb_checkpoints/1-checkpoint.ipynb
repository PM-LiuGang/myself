    "def count():\n",
    "    fs = []\n",
    "    for i in range(1, 4): # 返回闭包时牢记一点：返回函数不要引用任何循环变量，或者后续变化的变量\n",
    "        def f():\n",
    "            return i * i\n",
    "        fs.append(f)\n",
    "    return fs\n",
    "\n",
